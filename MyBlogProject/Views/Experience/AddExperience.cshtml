@using MyBlogDomain.Entities
@model Experience
@{
    ViewData["Title"] = "AddExperience";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Add Experience</h1>
<form class="form-control" method="post">
    <label asp-for="CompanyName">Company Name</label>
    <input type="text" asp-for="CompanyName" class="form-control" />
    <label asp-for="Address"></label>
    <input asp-for="Address" type="text" class="form-control" />
    <label asp-for="Title"></label>
    <input asp-for="Title" type="text" class="form-control" />
    <label asp-for="DescriptionOfRole"></label>
    <input asp-for="DescriptionOfRole" type="text" class="form-control" />
    <label asp-for="StartDate"></label>
    <input asp-for="StartDate" type="date" class="form-control" />
    <label asp-for="EndDate"></label>
    <input asp-for="EndDate" type="date" class="form-control" />
    <button type="submit" class="form-control"> Add</button>
</form>

